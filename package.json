{
  "name": "frontend-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server/dev-server.js",
    "dev:client": "vite",
    "build": "yarn build:client && yarn build:server",
    "build:server": "vue-tsc && vite build --ssr src/entry-server.ts",
    "build:client": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "serve": "node server/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "prepare": "husky",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:check": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "stylelint": "stylelint \"**/*.{css,vue}\" --fix",
    "stylelint:check": "stylelint \"**/*.{css,vue}\"",
    "lint:all": "yarn lint && yarn format && yarn stylelint",
    "lint:all:check": "yarn lint:check && yarn format:check && yarn stylelint:check"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.0",
    "compression": "^1.7.4",
    "daisyui": "^5.0.46",
    "express": "^4.18.2",
    "pinia": "^2.1.7",
    "serve-static": "^1.15.0",
    "vue": "^3.5.17",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.42.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^24.0.13",
    "@types/serve-static": "^1.15.5",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-vue": "^10.3.0",
    "happy-dom": "^13.3.8",
    "husky": "^9.1.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.21.1",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "3",
    "typescript": "^5.8.3",
    "vite": "^5.0.8",
    "vitest": "^1.2.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.1"
  }
}
