{
  "name": "frontend-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server/dev-server.js",
    "dev:client": "vite",
    "build": "yarn build:client && yarn build:server",
    "build:server": "vue-tsc && vite build --ssr src/entry-server.ts",
    "build:client": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "serve": "node server/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "dependencies": {
    "daisyui": "^5.0.46",
    "vue": "^3.5.17",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "express": "^4.18.2",
    "compression": "^1.7.4",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "@types/express": "^4.17.21",
    "@types/compression": "^1.7.5",
    "@types/serve-static": "^1.15.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "3",
    "typescript": "^5.8.3",
    "vite": "^5.0.8",
    "vue-tsc": "^3.0.1",
    "@vue/test-utils": "^2.4.4",
    "vitest": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "jsdom": "^24.0.0",
    "@playwright/test": "^1.42.1",
    "happy-dom": "^13.3.8"
  }
}
